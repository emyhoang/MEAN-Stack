<html>

<head>
  <title>Edit mongoose</title>
  <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/bootstrap.css') }}" />
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script type="text/javascript" src="/socket.io/socket.io.js"></script>
  <script type="text/javascript">
    $(document).ready(function () {

    });
  </script>
</head>

<body>
  <% if(messages.notUpdated) { %>
  <% for (var x of messages.notUpdated) { %>
  <h3>
    <%= x %>
  </h3>
  <% } %>
  <% } %>

  <div style='border: 2px blue solid; border-radius: 10px; min-height: 400px; width: 75%'>
    <h1>Edit mongoose:

    </h1>
    <form action='/mongooses/edit/<%=mongoose._id%>' method='post'>
      Name: <input type='text' name='name' value="<%=mongoose.name%>"> <br /><br />
      Favorite Food: <textarea name='favorite_food'> <%=mongoose.favorite_food%></textarea> <br /><br />
      <input type='submit' value='submit!' class='btn btn-success'><br /><br />
    </form>
  </div>
</body>



</html>